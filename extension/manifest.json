{
  "key": "",
  "name": "Unattended Remote",
  "manifest_version": 2,
  "version":"0.0.7",
  "homepage_url": "https://www.chromebeat.com",
  "options_page": "options.html",
  "browser_action": {
      "default_icon": "icons/icon128.png",
      "default_title": "OK Remote Dev"
  },
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "permissions": [
    "http://127.0.0.1/*",
    "identity", 
    "identity.email", 
    "background", 
    "storage",
    "power",
    "idle"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "oauth2": {
    "client_id": "849432840306-9dmb47cv37ti4lij2rolne3n7cepdr2u.apps.googleusercontent.com",
    "scopes": [
      "email","profile"
    ]
  }
}
